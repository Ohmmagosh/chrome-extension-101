{
    "name": "Side Panel",
    "description": "Base Level Extensions",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Click to open panel"
    },
    "side_panel": {
        "default_path": "side-panel.html"
    },
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "sidePanel"
    ],
    "icons": {
        "16": "side-panel.png"
    }
}